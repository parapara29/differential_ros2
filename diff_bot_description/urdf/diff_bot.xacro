    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/castor_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="lidar_1">
  <inertial>
    <origin xyz="8.881784197001253e-18 2.220446049250313e-18 0.00976987914267307" rpy="0 0 0"/>
    <mass value="0.19229515327817076"/>
    <inertia ixx="2.5e-05" iyy="2.5e-05" izz="3.8e-05" ixy="-0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.0 -0.055" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/lidar_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 -0.0 -0.055" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/lidar_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="imu_1">
  <inertial>
    <origin xyz="0.0875 -0.02 -0.0075" rpy="0 0 0"/>
    <mass value="0.003925"/>
    <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.0875 -0.02 -0.01" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/imu_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.0875 -0.02 -0.01" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/imu_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="right_wheel_1">
  <inertial>
    <origin xyz="0.0 0.012499999999999997 4.336808689942018e-18" rpy="0 0 0"/>
    <mass value="0.9658815551055745"/>
    <inertia ixx="0.000428" iyy="0.000758" izz="0.000428" ixy="-0.0" iyz="-0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.05 -0.05 -0.005" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/right_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.05 -0.05 -0.005" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/right_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="camera_1">
  <inertial>
    <origin xyz="-0.0049999999999999906 0.0 -6.938893903907228e-18" rpy="0 0 0"/>
    <mass value="0.029715375582669967"/>
    <inertia ixx="4e-06" iyy="0.0" izz="4e-06" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.1 -0.0 -0.055" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/camera_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.1 -0.0 -0.055" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/camera_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="camera_link_optical"></link>
 <joint name="camera_optical_joint" type="fixed">
        <parent link="camera_1"/>
        <child link="camera_link_optical"/>
        <origin xyz="0 0 0" rpy="${-pi/2} 0 ${-pi/2}"/>
    </joint>


<joint name="footprint_joint" type="fixed">
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <parent link="base_footprint"/>
  <child link="base_link"/>
</joint>

<joint name="Revolute_1" type="continuous">
  <origin xyz="0.05 -0.05 0.005" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="left_wheel_1"/>
  <axis xyz="0.0 1.0 0.0"/>
</joint>

<joint name="Rigid_3" type="fixed">
  <origin xyz="-0.06 0.0 -0.005" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="castor_wheel_1"/>
</joint>

<joint name="Rigid_4" type="fixed">
  <origin xyz="0.0 0.0 0.055" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="lidar_1"/>
</joint>

<joint name="Rigid_5" type="fixed">
  <origin xyz="-0.0875 0.02 0.01" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="imu_1"/>
</joint>

<joint name="Revolute_6" type="continuous">
  <origin xyz="0.05 0.05 0.005" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="right_wheel_1"/>
  <axis xyz="0.0 1.0 0.0"/>
</joint>

<joint name="Rigid_7" type="fixed">
  <origin xyz="-0.1 0.0 0.055" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="camera_1"/>
</joint>

<ros2_control name="GazeboSystem" type="system">
    <hardware>
      <plugin>gazebo_ros2_control/GazeboSystem</plugin>
    </hardware>
    <joint name="Revolute_1">
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    <joint name="Revolute_6">
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
  </ros2_control>

</robot>
