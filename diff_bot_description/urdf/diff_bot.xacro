    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.05 0.05 -0.005" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diff_bot_description)/meshes/left_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="castor_wheel_1">
  <inertial>
    <origin xyz="6.938893903907228e-18 -8.881784197001253e-18 -0.01124999999999999" rpy="0 0 0"/>
    <mass value="0.4439070419522377"/>
    <inertia ixx="0.000104" iyy="0.000104" izz="0.00016" ixy="-0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.06 -0.0 0.005" rpy="0 0 0"/>
